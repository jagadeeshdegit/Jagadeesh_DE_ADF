{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf1jaga"
		},
		"linkedserviceDL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedserviceDL'"
		},
		"linkedservice_Storageaccountadf1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservice_Storageaccountadf1'"
		},
		"ls_database_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_database'"
		},
		"ls_datalakeadfpro_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalakeadfpro'"
		},
		"linkedserviceDL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccountadf1.dfs.core.windows.net/"
		},
		"linkedservice_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"ls_database_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adf1server.database.windows.net"
		},
		"ls_database_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adf1db"
		},
		"ls_database_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adf1admin"
		},
		"ls_datalakeadfpro_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccountadf1.dfs.core.windows.net/"
		},
		"ls_restapi_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pokeapi.co/api/v2/pokemon"
		},
		"schedule_properties_scheduled_parameters_schema": {
			"type": "string",
			"defaultValue": "source"
		},
		"schedule_properties_scheduled_parameters_table": {
			"type": "string",
			"defaultValue": "Orders"
		},
		"schedule_properties_scheduled_parameters_backdate": {
			"type": "string",
			"defaultValue": "2025-09-24"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf1jaga_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkedserviceDL_accountKey": {
						"value": "[parameters('linkedserviceDL_accountKey')]"
					},
					"linkedservice_Storageaccountadf1_connectionString": {
						"value": "[parameters('linkedservice_Storageaccountadf1_connectionString')]"
					},
					"ls_database_password": {
						"value": "[parameters('ls_database_password')]"
					},
					"ls_datalakeadfpro_accountKey": {
						"value": "[parameters('ls_datalakeadfpro_accountKey')]"
					},
					"linkedserviceDL_properties_typeProperties_url": {
						"value": "[parameters('linkedserviceDL_properties_typeProperties_url')]"
					},
					"linkedservice_git_properties_typeProperties_url": {
						"value": "[parameters('linkedservice_git_properties_typeProperties_url')]"
					},
					"ls_database_properties_typeProperties_server": {
						"value": "[parameters('ls_database_properties_typeProperties_server')]"
					},
					"ls_database_properties_typeProperties_database": {
						"value": "[parameters('ls_database_properties_typeProperties_database')]"
					},
					"ls_database_properties_typeProperties_userName": {
						"value": "[parameters('ls_database_properties_typeProperties_userName')]"
					},
					"ls_datalakeadfpro_properties_typeProperties_url": {
						"value": "[parameters('ls_datalakeadfpro_properties_typeProperties_url')]"
					},
					"ls_restapi_properties_typeProperties_url": {
						"value": "[parameters('ls_restapi_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf1jaga_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"schedule_properties_scheduled_parameters_schema": {
						"value": "[parameters('schedule_properties_scheduled_parameters_schema')]"
					},
					"schedule_properties_scheduled_parameters_table": {
						"value": "[parameters('schedule_properties_scheduled_parameters_table')]"
					},
					"schedule_properties_scheduled_parameters_backdate": {
						"value": "[parameters('schedule_properties_scheduled_parameters_backdate')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf1jaga_ArmTemplate_0')]"
			]
		}
	]
}